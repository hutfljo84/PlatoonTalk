generator client {
  provider      = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          String @id @default(cuid())
  name        String
  description String
  image       String
  price       Int
  sku         String
}

model Roster {
  id          String @id @default(cuid())
  usergrp     Int
  sectionId   Int
  rank        String
  name        String
  email       String
  address1    String
  address2    String
  city        String
  state       String
  postal      Int
  phone       Int
}
